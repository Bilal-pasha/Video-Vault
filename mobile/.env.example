# Environment Variables for Chaajao Mobile App
# Copy this file to .env and fill in your actual values

# Environment (development | production)
EXPO_PUBLIC_ENV=development

# Show logger in development
EXPO_PUBLIC_SHOW_LOGGER=true

# Next.js Backend API Base URL (if using Next.js API routes)
EXPO_PUBLIC_Next_BASE_URL=

# Django API Base URL
EXPO_PUBLIC_DJANGO_API_BASE_URL=

# Google OAuth Configuration
EXPO_PUBLIC_GOOGLE_CLIENT_ID=
EXPO_PUBLIC_GOOGLE_CALLBACK_URL=
