{
  "expo": {
    "name": "Video Mobile Application",
    "slug": "video-mobile-application",
    "version": "1.0.0",
    "scheme": "video-mobile-application",
    "web": {
      "favicon": "./src/assets/images/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      "expo-router",
      "expo-web-browser"
    ],
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.video-mobile-application.app",
      "associatedDomains": [
        "applinks:online2.video-mobile-application.com"
      ],
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/android-icon-foreground.png",
        "backgroundImage": "./src/assets/images/android-icon-background.png",
        "monochromeImage": "./src/assets/images/android-icon-monochrome.png"
      },
      "package": "com.bilal_pasha.videomobileapplication",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "online2.video-mobile-application.com"
            },
            {
              "scheme": "video-mobile-application"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9fe4a6a9-8c26-45dc-913b-0a395233b9c6"
      }
    },
    "owner": "bilal_pasha"
  }
}
