{
  "expo": {
    "name": "Video Mobile Application",
    "slug": "video-mobile-application",
    "version": "1.0.0",
    "scheme": "video-mobile-application",
    "web": {
      "favicon": "./src/assets/images/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": ["expo-router", "expo-web-browser"],
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.video-mobile-application.app",
      "associatedDomains": ["applinks:online2.video-mobile-application.com"],
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/android-icon-foreground.png",
        "backgroundImage": "./src/assets/images/android-icon-background.png",
        "monochromeImage": "./src/assets/images/android-icon-monochrome.png"
      },
      "package": "com.video-mobile-application.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "online2.video-mobile-application.com"
            },
            {
              "scheme": "video-mobile-application"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "fc3c9412-84be-46b2-adc6-347d8d6f4a44"
      }
    },
    "owner": "video-mobile-application_technologies"
  }
}